{
  "expo": {
    "name": "Esports Center",
    "slug": "esports-center",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esportsstream.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.esportsstream.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "authSessionRedirectUrl": "http://localhost:8081/oauth"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "esports-center",
    "authSessionRedirectUrl": "esports-center://oauth",
    "extra": {
      "clerkPublishableKey": "@EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}